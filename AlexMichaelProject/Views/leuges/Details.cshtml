@model IEnumerable<AlexMichaelProject.Models.team>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div id="show">
        <div class="panel panel-danger">
            <div class="col-md-3" style="margin-top:20px">
                @{ Html.RenderAction("SideMenu", "Home");}
            </div>

            <div class="col-md-9">
             
                <table class="table">
                    <tr>
                        <th>Club Name</th>
                        <th>League</th>
                        <th>Home Stadium</th>
                    </tr>
                    @foreach (var item in Model)
            {
                        <tr>
                            <td>@Html.DisplayFor(modelItem => item.teamName)</td>
                            <td>@Html.DisplayFor(modelItem => item.leuge)</td>
                            <td>@Html.DisplayFor(modelItem => item.homeStadium)</td>
                        </tr>
                    }
                </table>
            </div>
        </div>
    </div>

</body>
</html>
