@model AlexMichaelProject.Models.manufacturer

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details</title>
</head>
<body>
    <div class="col-md-3" style="width:20%;float:left;">
        @{ Html.RenderAction("SideMenu", "Home");}
    </div>
    <div class="col-md-3" style="width:80%;float:left;margin-top:20px;">
        <h4>manufacturer @Model.mname</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt class="control-label col-md-2">Manufacturers:</dt>
            <dd>@Html.DisplayFor(model => model.mname)</dd>
            <dt class="control-label col-md-2">WebSite:</dt>
            <dd><a href=http://@Html.DisplayFor(model => model.msite)>@Html.DisplayFor(model => model.msite)</a></dd>
        </dl>
    </div>
@if ((String)Session["power"] == "admin")
{
    <p class="btn btn-primary btn-md" style="background-color:khaki; margin-left:50px;">
        @Html.ActionLink("Edit", "Edit", new { id = Model.mname })
    </p>
}
    <p class="btn btn-primary btn-md">
        @Html.ActionLink("Back to List", "Index")
    </p>
</body>
</html>
