@model IEnumerable<AlexMichaelProject.Models.manufacturer>

@{
//Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                Manufacturers
            </th>
            <th>
                WebSite
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.mname)
                </td>
                <td>
                    <a href=http://@Html.DisplayFor(modelItem => item.msite)>@Html.DisplayFor(modelItem => item.msite)</a>
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.mname }) |
                    @Html.ActionLink("Details", "Details", new { id = item.mname }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.mname })
                </td>
            </tr>
        }

    </table>
</body>
</html>
